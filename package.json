  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.1",
    "eslint": "^5.0.1",
    "eslint-plugin-babel": "^5.1.0",
